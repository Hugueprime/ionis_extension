{
    "manifest_version": 2,
    "name": "ext",
    "version": "0.1.0",
    "content_scripts":[
        {
            "matches":[
                "https://ionisx.com/courses/*",
                "https://courses.ionisx.com/*"
            ],
            "js": ["config.js", "dates.js", "content.js"],
            "css":["added.css", "changed.css"]
        }
    ],
    "browser_action":{
        "default_icon": "./icons/icon32.png",
        "default_popup": "popup.html"
    },
    "permissions": [
        "storage"
    ]
}